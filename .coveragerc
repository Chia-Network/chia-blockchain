[run]
branch=True
relative_files=True
source=
  chia
  tests
concurrency=multiprocessing
parallel=True

[paths]
chia = 
    chia
    */chia
tests = 
    tests
    */tests

[report]
precision = 1
exclude_lines =
  pragma: no cover
  abc\.abstractmethod
  typing\.overload
  ^\s*\.\.\.\s*$
  if typing.TYPE_CHECKING:
