    (defun generate_conds (amount depth atom)
        (if amount
            (c (list 1 (+ atom depth)) (generate_conds (- amount 1)))
            (list (list 1 atom))
        )
    )

    (defun loop_coins (id coins amount depth)
        (if coins
            (c (list id (c 1 (generate_conds amount depth)) 123 (list 0 (list 1))) (loop_coins (+ id 1) (- coins 1)))
