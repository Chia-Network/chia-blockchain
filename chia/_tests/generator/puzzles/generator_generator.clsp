(mod (amount)

    (defun generate_conds (pair_count)
        (if pair_count
            (c 
                (list 1 "static" pair_count) 
                (c 
                    (list 1 (+ "hello" pair_count)) 
                    (generate_conds (- pair_count 1))
                )
            )
            0
        )
    )

    (defun loop_coins (id amount conds)
        (if amount
            (c (list id (c 1 conds) 123 (list 0 (list 1))) (loop_coins (+ id 1) (- amount 1) conds))
            ()
        )   
    )
    ; main
    (list (loop_coins 0x0101010101010101010101010101010101010101010101010101010101010101 amount (generate_conds 50)))
)