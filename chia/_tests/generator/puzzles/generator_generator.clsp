(mod (DEPTH deserializer_mod (block1 block2 block3))

    (defun ladder_maker (depth a b)
        (if depth
            (c (ladder_maker (- depth 1) a b) (ladder_maker (- depth 1) b a))
            (c a b)
        )
    )

    (defun generate_conds (amount depth)
        (if amount
            (c (list 1 (ladder_maker depth amount (* 101 amount))) (generate_conds (- amount 1)))
            0
        )
    )

    (defun loop_coins (id coins amount depth)
        (if coins
            (c (list id (c 1 (generate_conds amount depth)) 123 (list 0 (list 1))) (loop_coins (+ id 1) (- coins 1)))
            ()
        )   
    )
    ; main
    (list (loop_coins 0x0101010101010101010101010101010101010101010101010101010101010101 1 1 DEPTH))
)