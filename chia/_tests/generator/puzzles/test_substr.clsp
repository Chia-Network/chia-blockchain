(mod (args)

    (defun substr_loop (atom amount)
        (if amount
            (c (substr atom 1) (substr_loop atom (- amount 1)))
            0
        )
    )

    ; main
    (list 
        (list 
            51 
            0x60bc5062a80c4363cbe881815300d349c5524d98e3502a016466d14d1f3f25d9 
            200 
            ; we put the work here otherwise it gets optimised away
            (substr_loop 0x7fff 1000000) 
        )
    )
)